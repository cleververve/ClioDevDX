<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Clio_Discount_per_Year__c</fullName>
    <description>Hidden, used to calculate the discount / per year, visible on the quote and quote template</description>
    <externalId>false</externalId>
    <formula>IF(SBQQ__ProrateMultiplier__c &gt;1, ( SBQQ__AdditionalDiscount__c / SBQQ__ProrateMultiplier__c ) , SBQQ__AdditionalDiscount__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Discount per Year</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
